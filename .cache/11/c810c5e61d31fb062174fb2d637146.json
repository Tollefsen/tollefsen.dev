dClip: 'text',\n    fontFamily: 'Questrial, sans-serif',\n    '& a': {\n      textDecoration: 'none',\n      color: 'unset'\n    },\n    display: 'flex',\n    flexDirection: 'column',\n    justifyContent: 'space-between'\n  },\n  number: {\n    display: 'flex',\n    justifyContent: 'space-between',\n    padding: '8px 12px 0 12px'\n  },\n  name: {\n    textAlign: 'center',\n    padding: '60px 0 0 0',\n    fontWeight: 'strong'\n  },\n  position: {\n    textAlign: 'center',\n    fontSize: '12px'\n  },\n  contact: {\n    margin: 'auto 0'\n  },\n  location: {\n    fontSize: '10px',\n    alignSelf: 'flex-end'\n  }\n});\n\n// smallcaps\n// debossed\n\nconst EmbossedText = ({ children }) => {\n  const classes = useContext(CardApi).classes;\n  return <div className={classes.embossed}>{children}</div>;\n};\n\nconst CardApi = createContext(null);\n\nconst Card = () => {\n  const classes = useStyles();\n  const api = { classes };\n  return (\n    <CardApi.Provider value={api}>\n      <div className={classes.card}>\n        <EmbossedText>\n          <div className={classes.number}>\n            <a target=\"_blank\" href=\"https://www.buymeacoffee.com/lmqcopM\">\n              Buy me a coffee\n            </a>\n          </div>\n          <div className={classes.name}>\n            <span style={{ fontVariant: 'small-caps' }}>Håvard</span> TOLLEFSEN\n          </div>\n          <div className={classes.position}>DEVELOPER</div>\n        </EmbossedText>\n      </div>\n    </CardApi.Provider>\n  );\n};\n\nexport { Card };\n"},"lineCount":null}},"hash":"0f3c786f9843907c79167279482d0d3b","cacheData":{"env":{}}}